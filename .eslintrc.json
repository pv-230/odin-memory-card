{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": 0,
    "import/extensions": [2, "never", { "jsx": "always" }],
    "jsx-a11y/label-has-associated-control": [1, { "assert": "either" }],
    "react/jsx-no-bind": 0
  }
}
